    CREATE TABLE `eod` (
        `uid` INTEGER PRIMARY KEY AUTOINCREMENT,
        `ticker` STRING NULL,
        `date` DATE NULL,
        `open` FLOAT64 NULL,
        `close`FLOAT64 NULL,
        `high` FLOAT64 NULL,
        `low` FLOAT64 NULL,
        `adj_close` FLOAT64 NULL,
        `volume` FLOAT64 NULL,
        UNIQUE(ticker, date)
    );

    CREATE TABLE `tickers`(
        `ticker` STRING PRIMARY KEY,
        `name` STRING NULL,
        `exchange` STRING NULL, 
        `category name` STRING NULL, 
        `country` STRING NULL
    );
    
.mode csv
.import tickers.csv tickers